void enqueue(node *head) {
  mutex.lock();
  mode *ptr;
  for (ptr = head; ptr->next != NULL; ptr = ptr->next) {
  }

  ptr->next = new element();
  new_element->next = NULL;
  mutex.unlock();
}

node *dequeue() {
  node *ptr = NULL;
  mutex.lock();
  if (head->next != NULL) {
    ptr = head->next;
    head->next = head->next->next;
  }
  mutex.unlock();
  return ptr;
}
