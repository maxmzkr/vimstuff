#include <iostream>
#include "./vm_app.h"

int main() {
  char *p0 = reinterpret_cast<char *>(vm_extend(0));
  char *p1 = reinterpret_cast<char *>(vm_extend(0));
  char *p2 = reinterpret_cast<char *>(vm_extend(0));

  p0[0] = '0';
  std::cout << p1[0] << std::endl;

  p2[0] = '2';
  p1[0] = '0';
  p0[0] = '0';
}
