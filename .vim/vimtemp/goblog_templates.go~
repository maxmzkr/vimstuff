package goblog

import (
  "github.com/maxmzkr/gourlparser"
  "github.com/maxmzkr/goblog/handlers"
)

var gourlParserTemplates = []gourlparser.Template{
  gourlparser.Template{
    Path: "/",
    Handler: handlers.RootHandler,
  },
}

var gourlParsers = gourlparser.ParseTemplateArray(gourlParserTemplates)
