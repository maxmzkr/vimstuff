#include <iostream>

int main() {
  unsigned int vm_page_size = 4096;
  void* message = reinterpret_cast<void*>(0x60001002);
  void* intptr = reinterpret_cast<void*>(reinterpret_cast<char*>(message) /
                                         vm_page_size * vm_page_size);
  std::cout << intptr << std::endl;
}
