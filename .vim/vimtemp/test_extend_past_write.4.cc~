#include "./vm_app.h"

int main() {
    char *p;
    p = reinterpret_cast<char *>(vm_extend(0));
    p[0] = 'h';
    p[1] = 'e';
    p[2] = 'l';
    p[3] = 'l';
    p[4] = 'o';
    vm_syslog(p, 10);
    p = reinterpret_cast<char *>(vm_extend(0));
    *(p - 1) = 'a';
    p[0] = 'b';
    vm_syslog(p - 1, 2);
    p = reinterpret_cast<char *>(vm_extend(0));
    p[0] = 'l';
    vm_syslog(p - 1, 2);
    p = reinterpret_cast<char *>(vm_extend(0));
    p[0] = 'a';
    vm_syslog(p - 1, 2);
    p = reinterpret_cast<char *>(vm_extend(0));
    p[0] = 'h';
    vm_syslog(p - 1, 2);
}
