#include "./share_table.h"

Pie * ShareTable::get_pie(unsigned int share_id) {
  std::unordered_map<unsigned int, Pie*>::iterator share_pie = pie_list_.find(share_id);
  if (share_pie == pie_list_.end()) {
    return nullptr;
  }
  return share_pie->second;
}

void ShareTable::set_pie(unsigned int share_id, Pie* pie) {
  pie_list_[share_id] = pie;
}

void ShareTable::delete_pie(unsigned int share_id) {
  pie_list_.erase(share_id);
}

std::unordered_map<unsigned int, Pie*> ShareTable::pie_list_;
